package fundamentos;

import javax.swing.JOptionPane;

public class ExercicioPabloTernarioDinamico {
	public static void main(String[] args) {
		String nota = JOptionPane.showInputDialog("Qual é a Nota ? ");
		
		
		
		if (Double.parseDouble(nota) >= 7)
			resultado = "Aprovado";
		else
			resultado = "Reprovado";
		
		
	}

}
