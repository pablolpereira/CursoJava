package fundamentos;

public class OperadoresUnarios {
	public static void main(String[] args) {
		int num1 = 1 ; 
		int num2 = 2 ; 
		
		//incremento e decremento
		//incremento
		num1++;
		System.out.println(num1); //Resultado 2
		
		//Decremento
		--num1;
		System.out.println(num1); //Resultado volta a ser 1
	}

}
