package fundamentos;

public class TiposPrimetivos {
	public static void main(String[] args) {
		//Tipos Primitivo boolenano
		boolean bo1 = false;
		boolean bo2 = true ;
		System.out.printf("%b %b\n", bo1 , bo2);
		
		//Tipo primitivo caracter
		char c1 = 'a';
		char c2 = '!';
		char c3 = '\u0050';
	}

}
